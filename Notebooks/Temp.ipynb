    "np.random.seed(0)\n",
    "\n",
    "T_x = np.linspace(0, 15, 1000)[:, np.newaxis]\n",
    "T_y = np.sin(T_x).ravel()\n",
    "\n",
    "X = np.sort(15 * np.random.rand(150, 1), axis=0)\n",
    "y = np.sin(X).ravel()\n",
    "\n",
    "# Add noise to targets\n",
    "y += 1.5 * (0.5 - np.random.rand(150))\n",
    "\n",
    "plt.plot(T_x,T_y,c='g')\n",
    "plt.scatter(X,y)"
